<div class="user-tabs">
  <a class="tab active"><i class="fa fa-th"></i>POSTS</a>
</div>

<div class="user-images">
  <% @posts.each do |post|%>
    <div class="wrapper">
      <%=link_to post_path(post) do %>
        <%if post.image.attached?%>
          <%= cl_image_tag(post.image.key, width: 700, height: 550, class:'border') %>
        <% end %>
      <% end %>
    </div>
  <% end %>
</div>
